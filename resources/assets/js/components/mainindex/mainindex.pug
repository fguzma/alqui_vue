    // Slide for show a preview of services.
    #gallery
        b-carousel#gallery-carousel( controls=true, background="#ababab", :interval="1500", sv-model="slide", @sliding-start="onSlideStart", @sliding-end="onSlideEnd", img-width="400", img-height="400")
            b-carousel-slide(v-for="media in Media" :key= "media.id" :img-src="media.images[0].url")
