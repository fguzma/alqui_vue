#main
    //video header and navigation divs.
    .video-container#header
        video(autoplay=true, height="720", loop=true, poster="img/wine.jpg", width="1280")
            source(src="video/mp4/wine.mp4" type="video/mp4")
            source(src="video/webm/wine.webm" type="video/webm")
    .container-fluid.no-padding.no-margin.relative.h-100#home
        nav.fixed.top.right.navigation.white-text.row.full-width.up#navigation
            .col-12
                ul.list-inline.float-right.row
                    li.list-inline-item.col-12.col-md-1.col-lg-1.nav-icon
                        a.icon#button
                            font-awesome-icon#menu-icon(:icon="['fas', 'bars']")
                    li.list-inline-item.col-12.col-md-2.col-lg-2
                        a(href="#header") Inicio
                    li.list-inline-item.col-12.col-md-2.col-lg-2
                        a(href="#menu") Servicios
                    li.list-inline-item.col-12.col-md-2.col-lg-2
                        a(href="#gallery") Galeria
                    li.list-inline-item.col-12.col-md-2.col-lg-2
                        a(href="#contact-form") Contacto
                    li.list-inline-item.col-12.col-md-3.col-lg-3
                        a(href="#location") Como llegar
        .row.no-padding.h-100
            .white-text.col-12.h-100.no-padding.text-center#parent
                .h-100#child
                    img(src="img/logo.png" width="200")
                    h1.satisfy.title.no-margin Alquileres Santana
                    p#is-open
                        i.fa.fa-clock-o Abierto Ahora
        // Barra de navegacion sticky
        .black.large-padding.white-text.full-width.absolute.up#description
            .row
                #description-text.col-6.white-text.medium.mx-auto.pt-2.text-center Alquileres Santana
                .col-6.hidden#sticky-navigation
                    nav.text-right.navigation.row
                        ul.list-inline.row
                            li.list-inline-item.col-12.col-md-1.col-lg-1.nav-icon
                                a.responsive-button.icon#button-2
                                    font-awesome-icon(:icon="['fas', 'bars']")
                            li.list-inline-item.col-12.col-md-2.col-lg-2
                                a(href="#header") Inicio
                            li.list-inline-item.col-12.col-md-2.col-lg-2
                                a(href="#menu") Servicios
                            li.list-inline-item.col-12.col-md-2.col-lg-2
                                a(href="#gallery") Galeria
                            li.list-inline-item.col-12.col-md-2.col-lg-2
                                a(href="#contact-form") Contacto
                            li.list-inline-item.col-12.col-md-3.col-lg-3
                                a(href="#location") Como llegar
    // Breve descripcion de los servicios de la empresa
    .text-center.relative#menu
        .absolute#menu-title
            h2.white-text.newblack.satisfy.title#service  Servicios
        .row
            .col-12.col-md-3.col-lg-3.services(v-for="media in Media" :key="media.id" v-bind:style="{'background-image': 'url(' + media.images[0].url + ')' }")
                .screen
                    .row.full-height.t-div
                        .col-12.white-text.t-child
                            h3.satisfy.subtitle(v-bind:id="media.slug") {{ media.name }}
    b-modal#modal1(href="modal1")
        b-carousel(controls=true, background="#ababab", :interval="1500", v-model="slide", @sliding-start="onSlideStart", @sliding-end="onSlideEnd", img-width="400", img-height="480")
